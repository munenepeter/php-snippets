	"Add Tailwind css":{
		"scope": "javascript,html,php",
		"prefix": "tailwind",
		"body": "<script src=\"https://cdn.tailwindcss.com\"></script>\n",
		"description": "Add the play cdn for Tailwind"
	},
	"Add Flowbite css & js":{
		"scope": "javascript,html,php",
		"prefix": "flowbite",
		"body": "<link rel=\"stylesheet\" href=\"https://unpkg.com/flowbite@1.5.3/dist/flowbite.min.css\" />\n<script src=\"https://unpkg.com/flowbite@1.5.3/dist/flowbite.js\"></script>\n",
		"description": "Add the flowbite assets"
	},
	"Add Axios js":{
		"scope": "javascript,html,php",
		"prefix": "axios",
		"body": "<script src=\"https://unpkg.com/axios/dist/axios.min.js\"></script>\n",
		"description": "Add the axios cdn"
	},
	"Add Vue js":{
		"scope": "javascript,html,php",
		"prefix": "vue",
		"body": "<script src=\"https://unpkg.com/vue@3/dist/vue.global.js\"></script>\n",
		"description": "Add the vue cdn"
	},
	"Add Apline js":{
		"scope": "javascript,html,php",
		"prefix": "alp",
		"body": "<script src=\"https://unpkg.com/alpinejs@3.x.x/dist/cdn.min.js\"></script>\n",
		"description": "Add the alpine cdn"
	},
	"Document design moode":{
		"scope": "html",
		"prefix": "des",
		"body": "<script>\n\n document.designMode = \"on\"; \n</script>\n\n",
		"description": "Add the alpine cdn"
	},
	"Red underline":{
		"scope": "css,html,php",
		"prefix": "red",
		"body": "\n\n p,h1,h2,h3,h4,h5{border-bottom: 1px dashed red;} \n\n",
		"description": "Add the alpine cdn"
	}


